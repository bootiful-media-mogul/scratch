
type Mogul {
 name :String
}

type Podcast {
    date : String
    description : String
    title : String
}

type PodcastDraft  {
    id : String
    completed: Boolean
    uid : String
    uploadPath: String
    pictureFileName: String
    introductionFileName: String
    interviewFileName: String
    title: String
    description: String
}

type Query {
    me: Mogul
    podcasts: [Podcast]
}

type Mutation {

    createPodcastDraft(uid:String) : PodcastDraft

}